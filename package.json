{
  "name": "ah-mixer-remote",
  "version": "0.1.0",
  "description": "Allen & Heath Digital Mixer Remote Lib",
  "scripts": {
    "test": "mocha -r ts-node/register 'test/**/*.ts'",
    "test-integrated": "mocha -r ts-node/register testIntegrated/**/*.ts",
    "coverage": "nyc npm run test",
    "build": "rimraf build && tsc",
    "start": "nodemon --exec ts-node index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xDGeForcexD/ah-mixer-remote.git"
  },
  "keywords": [],
  "author": "xDGeForcexD <info@xdgeforcexd.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xDGeForcexD/ah-mixer-remote/issues"
  },
  "homepage": "https://github.com/xDGeForcexD/ah-mixer-remote#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "chai": "^4.2.0",
    "mitm": "^1.7.1",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "rewire": "^5.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "net": "^1.0.2"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "index.ts"
    ],
    "exclude": [
      "test/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
